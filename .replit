modules = ["nodejs-20", "web", "postgresql-16", "python-3.11"]
[agent]
expertMode = true
integrations = ["javascript_object_storage:1.0.0", "google-mail:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=production npm run server"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3001
externalPort = 3001
exposeLocalhost = true

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 37917
externalPort = 3003

[[ports]]
localPort = 40159
externalPort = 3002

[[ports]]
localPort = 44607
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["bash", "-c", "NODE_ENV=production npm run server"]
build = ["npm", "run", "build"]

[objectStorage]
defaultBucketID = "replit-objstore-2c936138-a04f-4c6d-b6b4-4b227e055990"
